{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "moduleResolution": "node"
    
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ]
}